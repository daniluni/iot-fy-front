<script type="text/javascript">

	window.onload = function () {

      var dps = [];   //dataPoints. 
                var elemento=[];
                {{#each consumos}}
                    var idajena = "{{fecha}}";
                    var WattsAjeno = {{Wh}};
                    elemento={y:WattsAjeno, label:idajena };
                    dps.push(elemento);
                {{/each}}
     console.log(dps);
      var chart = new CanvasJS.Chart("chartContainer",{
      	title :{
      		text: "Consumos Acumulados x Fecha"
      	},
      	axisX: {						
      		title: "Fecha"
      	},
      	axisY: {						
      		title: "Watts por hora"
      	},
      	data: [{
      		type: "bar",
      		dataPoints : dps
      	}]
      });

      chart.render();

        };

</script>
</head>
<body>
<div id="chartContainer" style="height: 300px; width: 100%;"></div>